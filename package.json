{
  "name": "our-new-bridge",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/prisma-generate.mjs && next build",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:schema": "dotenv -e .env.local -- node scripts/prisma-create-schema.mjs",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "migrate:json": "dotenv -e .env.local -- ts-node prisma/migrate-from-json.ts",
    "start": "next start",
    "lint": "eslint",
    "validate": "node scripts/validate-config.mjs"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.4.2",
    "@chakra-ui/react": "^3.33.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@prisma/client": "4.16.2",
    "@react-google-maps/api": "^2.20.8",
    "framer-motion": "^12.34.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-auth": "^4.24.7",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-leaflet": "^5.0.0",
    "resend": "^6.9.1"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^6.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "4.16.2",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
